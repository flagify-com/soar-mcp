# SOAR 平台配置
API_URL=https://your-soar-platform.com
API_TOKEN=your_jwt_token_here
SSL_VERIFY=0  # 0=禁用SSL验证（默认），1=启用SSL验证

# MCP 服务器配置
MCP_PORT=12345
ADMIN_PORT=12346

# 数据库配置
